# EditorConfig: http://EditorConfig.org
root = true

[*]
end_of_line = lf
insert_final_newline = false
trim_trailing_whitespace = true
indent_style = space
indent_size = 2

# Matches multiple files with brace expansion notation
# Set default charset
[*.{js<Plug>PeepOpeny}]
charset = utf-8

[*.http]
ignore = true
